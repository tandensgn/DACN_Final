                    <ul>
                        <li class="mb-3">
                            <i class="fas fa-map-marker"></i>
                            331, Highway 1A, An Phu Dong Ward, District 12, Ho Chi Minh City.
                        </li>
                        <li class="mb-3">
                            <i class="fas fa-mobile"></i> 028 000 0001
                        </li>
                        <li class="mb-3">
                            <i class="fas fa-phone"></i> +849 00 000 001
                        </li>
                        <li class="mb-3">
                            <i class="fas fa-envelope-open"></i>
                            <a href="mailto:email1@gmail.com">email1@gmail.com</a>
                        </li>
                    </ul>
                </div>
                <div class="col-md-3 col-sm-6 footer-grids w3l-agileits mt-md-0 mt-4">

                    <!-- social icons -->
                    <div class="footer-grids  w3l-socialmk md-3">
                        <h3 class="text-white font-weight-bold mb-3">Follow Us on</h3>
                        <div class="social">
                            <ul>
                                <li>
                                    <a class="icon fb" href="https://www.facebook.com/">
                                        <i class="fab fa-facebook-f"></i>
                                    </a>
                                </li>
                                <li>
                                    <a class="icon tw" href="https://www.instagram.com/">
                                        <i class="fab fa-twitter"></i>
                                    </a>
                                </li>
                                <li>
                                    <a class="icon gp" href="https://twitter.com/">
                                        <i class="fab fa-google-plus-g"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- //social icons -->
                </div>
            </div>
            <!-- //quick links -->
        </div>
    </div>
